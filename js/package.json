{
  "name": "unray",
  "version": "0.1.0",
  "description": "Jupyter Widget for Volume Rendering of Unstructured Tetrahedral Mesh Data",
  "author": "Martin Sandve Aln√¶s <martin.alnes@gmail.com>",
  "license": "BSD-3-Clause",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martinal/unray.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "visualization",
    "rendering",
    "scientific",
    "FEM",
    "webgl",
    "threejs"
  ],
  "scripts": {
    "build": "webpack",
    "lint": "eslint src/*.js",
    "node": "node",
    "watch": "onchange 'package.json' 'src/*.js' 'src/glsl/*.glsl' 'src/glsl/utils/*.glsl' -v -- npm run build",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:watch": "karma start --browsers ChromeHeadless karma.conf.js",
    "test:single": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:chrome": "karma start --single-run --browsers Chrome karma.conf.js",
    "test:firefox": "karma start --single-run --browsers Firefox karma.conf.js",
    "test:debug": "karma start --debug=true --browsers Chrome karma.conf.js"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.7.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "onchange": "^3.2.1",
    "webpack": "^3.6.0",
    "webpack-glsl-loader": "^1.0.1"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.2",
    "d3-array": "^1.2.0",
    "d3-color": "^1.0.3",
    "d3-scale": "^1.0.6",
    "gl-matrix": "^2.4.0",
    "isndarray": "^1.0.0",
    "jupyter-datawidgets": "^2.1.0",
    "ndarray": "^1.0.18",
    "ndarray-determinant": "^1.0.0",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.2.1",
    "three": "^0.87.1",
    "underscore": "^1.8.3"
  }
}
